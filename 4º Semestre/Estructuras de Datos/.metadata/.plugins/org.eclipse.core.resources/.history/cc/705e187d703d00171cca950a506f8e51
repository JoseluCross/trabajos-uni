		Nodo<T> der, izq;
		T elemento;
		
		public Nodo<T> getDer(){ return this.der; }
		public Nodo<T> getIzq(){ return this.izq; }
		public T getDato() { return this.elemento; }
	}
	
	public List<E> nodosHoja() {
		
	}
	
	private Nodo<E>[] nodosToArray(){
		Nodo[] nodos = new Nodo[size];
		Stack<Nodo> pilaNodos = new Stack<>();
		
	} 

}
